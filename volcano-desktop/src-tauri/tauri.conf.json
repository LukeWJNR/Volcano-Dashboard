{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "volcano-desktop",
  "version": "0.1.0",
  "identifier": "com.volcano-desktop.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
  {
    "label": "main",
    "url": "http://localhost:8501",
    "fullscreen": false,
    "resizable": true,
    "title": "Volcano Dashboard"
  }
],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
